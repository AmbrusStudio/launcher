# Ambrus Studio main site url
VITE_MAIN_SITE_URL=https://dev.ambrus.studio

# Ambrus Studio backend api url
VITE_BACKEND_API_URL=https://api.ambrus.studio

# Ambrus Studio account service backend api base path
VITE_ACCOUNT_BACKEND_API_PATH=/account-dev

# Set chain id
VITE_CHAIN_ID=5

# Your Infura api key
VITE_INFURA_API_KEY=

# Your Etherscan api key
VITE_ETHERSCAN_API_KEY=

# Your alchemy api key
VITE_ALCHEMY_API_KEY=

# ImmutableX link connection url https://docs.x.immutable.com/docs/immutable-x-sdk/#setting-the-connection-urls
# Mainnet is https://link.x.immutable.com
VITE_IMX_LINK_URL=https://link.sandbox.x.immutable.com

# ImmutableX api connection url https://docs.x.immutable.com/docs/immutable-x-sdk/#setting-the-connection-urls
# Mainnet is https://api.x.immutable.com/v1
VITE_IMX_API_URL=https://api.sandbox.x.immutable.com/v1

# Polygon information
VITE_ALCHEMY_POLYGON_API_KEY=
VITE_POLYGON_NETWORK=polygon-mainnet

# Diamond Hand SBT contract address
VITE_DIAMOND_HAND_CONTRACT_ADDRESS=